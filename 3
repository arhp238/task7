#include <iostream>
#include <vector>

using namespace std;

int main() {
    vector<int> arr = {-2, 1, -3, 4, -1, 2, 1, -5, 4};

    int maxSum = arr[0]; 
    int currentSum = arr[0]; 

    for (int i = 1; i < arr.size(); i++) {
        if (currentSum < 0) {
            currentSum = arr[i];
        } else {
            currentSum += arr[i]; 
        }

        if (currentSum > maxSum) {
            maxSum = currentSum;
        }
    }

    cout << maxSum << endl;

    return 0;
}
